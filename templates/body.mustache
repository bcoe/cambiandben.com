<!-- custom CSS -->
<link rel="stylesheet" href="custom.css">

<!-- main JS libs  -->
<script src="js/libs/modernizr-2.5.3.min.js"></script>
<script src="js/libs/respond.min.js"></script>
<script src="js/libs/jquery.min.js"></script>
<script src="js/libs/handlebars.min.js"></script>
<script src="js/jquery.customInput.js"></script>
<script src="js/countdown.js"></script>

<!-- scripts  -->
<script src="js/jquery.easing.1.3.min.js"></script>
<script src="js/general.js"></script>

<!-- shortcode: Lightbox prettyPhoto -->
<link rel="stylesheet" href="css/prettyPhoto.css">
<script src="js/jquery.prettyPhoto.js"></script>

<!-- carousel -->
<script src="js/jquery.carouFredSel.min.js"></script>

</head>
<body>

<div class="body_wrap">
  <div class="main_outer">
 	  <div class="main_top"></div>
     <div class="main_mid">

        <!-- header -->
        <div class="header">

        	<div class="head_title">
          	<h1>We&rsquo;re Getting Married</h1>
            <a href="http://www.google.com/calendar/event?action=TEMPLATE&text=Cambi%20%26%20Ben's%20Wedding&location=Mann%20Family%20Farm%2C%20605%20Horseshoe%20Hill%20Road%2C%20Bolinas%2C%20California%2094924&details=http%3A%2F%2Fwww.cambiandben.com&dates=20151023T230000Z/20151023T290000Z" target="_blank"><p class="sub_title">Friday, 23<sup>rd</sup> October 2015</p></a>
          </div>

          <div class="head_names">
          	<div class="head_name_left">
          	  <span class="head_name">
              	<strong>CAMBI</strong> <em>EVERS-EVERETTE</em>
              </span>
              </div>
              <span class="head_amp"><img src="styles/beige-turqoise/head_amp.png" width="125" height="122" alt=""></span>
              <div class="head_name_right">
              <span class="head_name">
              	<strong>BENJAMIN</strong> <em>COE</em>
              </span>
            </div>

            <span class="ribbon_left"></span>
            <span class="ribbon_right"></span>
          </div>

        </div>
        <!-- header -->

        <!-- topmenu -->
		    <div class="topmenu_line_top"></div>
        <nav id="topmenu">
          <ul class="dropdown">
            <li class="menu-level-0 {{#if isIndex}}current-menu-item{{/if}}"><a href="/"><span>HOME</span></a></li>
          	<li class="menu-level-0 {{#if isDayOf}}current-menu-item{{/if}}"><a href="/dayof"><span>DAY OF</span></a></li>
            <li class="menu-level-0 {{#if isAccomodation}}current-menu-item{{/if}}"><a href="/accomodation"><span>ACCOMODATION</span></a></li>
            <li class="menu-level-0 {{#if isRSVP}}current-menu-item{{/if}}"><a href="/rsvp"><span>RSVP</span></a></li>
            <li class="menu-level-0 {{#if isRegistry}}current-menu-item{{/if}}"><a href="/registry"><span>REGISTRY</span></a></li>
            <li class="menu-level-0 {{#if isGallery}}current-menu-item{{/if}}"><a href="/gallery"><span>GALLERY</span></a></li>
          </ul>
        </nav>
        <div class="topmenu_line_bot"></div>
       <!--/ topmenu -->

       <!-- content -->
	     <div class="content post-detail">

        	<div class="entry">
            {{#if isIndex}}{{> indexBody}}{{/if}}
            {{#if isDayOf}}{{> dayOfBody}}{{/if}}
            {{#if isAccomodation}}{{> accomodationBody}}{{/if}}
            {{#if isRSVP}}{{> rsvpBody}}{{/if}}
            {{#if isRegistry}}{{> registryBody}}{{/if}}
            {{#if isGallery}}{{> galleryBody}}{{/if}}
            {{#if isAdmin}}{{> adminBody}}{{/if}}
          </div>
          <!--/ content -->

      </div>
    <div class="main_bot"></div>
  </div>
<!--/ container -->

  <footer>
  	<div class="container">
     		<p class="copyright">The future estate of Cambi and Ben<span class="icon_heart">&hearts;</span> <a href="http://www.google.com/calendar/event?action=TEMPLATE&text=Cambi%20%26%20Ben's%20Wedding&location=Mann%20Family%20Farm%2C%20605%20Horseshoe%20Hill%20Road%2C%20Bolinas%2C%20California%2094924&details=http%3A%2F%2Fwww.cambiandben.com&dates=20151023T230000Z/20151023T290000Z" target="_blank">October 23<sup>rd</sup> 2015</a></p>
  	</div>
  </footer>

  <script id="guest-row-template" type="text/x-handlebars-template">
    <li class="guest-row"><span>\{{email}}</span><span>\{{food}}</span><span>\{{#unless isGuest}}(<a href="#" class="delete-link" data-email="\{{email}}">delete</a>)\{{/unless}}</span></li>
  </script>

  <script id="rsvp-lookup-template" type="text/x-handlebars-template">
    <form action= "#" method="post" id="email-lookup-form">

       <div class="rowSubmit field_text aligncenter">
         <label class="label_title"><strong>Email:</strong></label>
         <input type="text" name="email" id="email" value="" class="inputtext input_middle required">
       </div>

       <div class="clear"></div>

       <div class="row rowSubmit">
       <input type="submit" id="rsvp-lookup" name="btnsubmit" value="Lookup Invite" class="btn-submit">
       </div>
    </form>
  </script>

  <script id="rsvp-update-template" type="text/x-handlebars-template">
     <form action= "#" method="post" id="rsvp-update-form" style="display: none;">

       <div class="row alignleft input_styled inlinelist omega" id="rsvp-attending">
         <div class="rowRadio"><input type="radio" name="attending" value="yes" id="radio_v1" checked> <label for="radio_v1">YES, I AM ATTENDING</label></div>
         <div class="rowRadio"><input type="radio" name="attending" value="no" id="radio_v2"> <label for="radio_v2">NO, Iâ€™M NOT ATTENDING</label></div>
       </div>

       <div id="rsvp-additional-questions">
         <div class="row field_text alignleft">
             <label class="label_title"><strong>First name:</strong></label>
             <input type="text" name="fname" id="fname" value="" class="inputtext input_middle required">
         </div>

         <div class="row field_text alignleft omega">
             <label class="label_title"><strong>Last name:</strong></label>
             <input type="text" name="lname" id="lname" value="" class="inputtext input_middle required">
         </div>

         <div class="row alignleft input_styled inlinelist omega">
           <div class="rowRadio"><input type="radio" name="food" value="beef" id="radio_v1" checked> <label for="radio_v1">BEEF</label></div>
           <div class="rowRadio"><input type="radio" name="food" value="chicken" id="radio_v2"> <label for="radio_v2">CHICKEN</label></div>
           <div class="rowRadio"><input type="radio" name="food" value="vegetarian" id="radio_v2"> <label for="radio_v2">VEGETARIAN</label></div>
         </div>

         <div class="clear"></div>

         \{{#if plusOneAllowed}}

         <div class="row field_text alignleft">
             <label class="label_title"><strong>Guest First name:</strong></label>
             <input type="text" name="guestfname" id="guestfname" value="" class="inputtext input_middle required">
         </div>

         <div class="row field_text alignleft omega">
             <label class="label_title"><strong>Guest Last Name:</strong></label>
             <input type="text" name="guestlname" id="guestlname" value="" class="inputtext input_middle required">
         </div>

         <div class="row alignleft input_styled inlinelist omega">
           <div class="rowRadio"><input type="radio" name="guestfood" value="beef" id="radio_v1" checked> <label for="radio_v1">BEEF</label></div>
           <div class="rowRadio"><input type="radio" name="guestfood" value="chicken" id="radio_v2"> <label for="radio_v2">CHICKEN</label></div>
           <div class="rowRadio"><input type="radio" name="guestfood" value="vegetarian" id="radio_v2"> <label for="radio_v2">VEGETARIAN</label></div>
         </div>
         \{{/if}}
       </div>

       <div class="clear"></div>

       <input type="hidden" name="email" value="\{{email}}" />

       <div class="row rowSubmit">
         <input type="submit" id="send" name="btnsubmit" value="RSVP NOW" class="btn-submit">
       </div>
    </form>
  </script>

  </div>
</body>
</html>
